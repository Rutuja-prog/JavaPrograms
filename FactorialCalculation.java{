public class FactorialCalculation {

    public static void main(String[] args) {
        int number = 6; // The number for which to calculate the factorial
        long factorial = 1; // Initialize factorial to 1

        // Loop to calculate the factorial
        for (int i = 1; i <= number; i++) {
            factorial *= i; // Multiply factorial by the current number in the loop
        }

        System.out.println("The product of 1*2*3*4*5*6 is: " + factorial);
    }
}